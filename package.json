{
  "name": "tracr.io",
  "version": "0.1.0",
  "description": "An app for tacking lost items",
  "main": "src/electron-app/main.js",
  "proxy": "http://localhost:3000",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "ts-node src/server/index.ts",
    "electron": "electron .",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "next lint",
    "prep": "prettier --config .prettierrc ./**/*.ts* --write && eslint . --ext .ts & eslint . --ext .tsx",
    "lint-fix": "eslint . --ext .ts --fix && eslint . --ext .tsx --fix",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts* --write",
    "start-api": "node dist/api/index.js",
    "start-api-dev": "npx ts-node api/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "electron": "^30.0.8",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "dotenv": "^16.4.5",
    "electron": "^30.0.8",
    "express": "^4.19.2"
  }
}
